generator client {
  provider = "prisma-client"
  output = "../prisma-pc04-database/pc04-database-client-types"
}

datasource db {
  provider = "sqlserver"
}

model FuelTimeLoss_1 {
  RecordId     Int     @id(map: "PK__TruckLos__FBDF78E90F673081") @default(autoincrement())
  DateYear     Int?
  DateMonth    String? @db.VarChar(24)
  OperatorName String? @db.VarChar(40)
  Crew         String? @db.VarChar(12)
  Reason       String? @db.VarChar(255)
  LossFuel     Float?
  LossTime     Float?
}

